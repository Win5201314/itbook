/* #comm style
================================================== */
html,body {background-color: #f1f1f1;height: 100%;overflow: hidden;position: relative;-webkit-overflow-scrolling: touch;}
.all-conter{margin:0 0.25rem;}
.i_main{position: absolute;top: 0rem;right: 0;bottom: 0rem;left: 0;width: 100%;z-index: 1;}
#i_wrap,.bmBarEle,.mbar_shadow {-webkit-transition: all .3s ease;-o-transition: all .3s ease;transition: all .3s ease;}
#i_wrap {position: relative;height: 100%;}
#wrapper,.i_sort_mainEle {position: absolute;z-index: 3;top: 0;bottom: 0;left: 0;right: 0;width: 100%;overflow: auto;}
#wrapper>.scroller,.i_sort_mainEle>.scroller {position: absolute;z-index: 1;width: 100%;padding: 0;-webkit-tap-highlight-color: rgba(0,0,0,0);}
.i_header{position: relative;top:0;z-index: 9999999;}
.i_h_bx{background-color: #38404e;height: 1.2rem;padding: 0 0.375rem;position: relative;clear: both;}
.i-logo{float: left;width: 2.5rem;height: 1.2rem;}
.i-logo img{display: inline-block;vertical-align: middle;max-width: 100%;height: auto;margin-top: 0.07rem;}
.i_cm_box {background: #fff;border: solid #e6e6e6;border-width: 1px 0;margin-top: 0.25rem;padding: 0.25rem;}
.i_cm_box.i_cm_box_top{margin-top: 0;}
.i_main {position: absolute;top: 1.2rem;right: 0;bottom: 1.2rem;left: 0;width: 100%;z-index: 1;}
.i_mainEle {position: relative;width: 100%;height: 100%;}
.i_cm_small_title {border-bottom: 0.025rem solid #e2e2e2;height: 0.4rem;padding: 0.25rem 0;overflow: hidden;clear: both;}
.i_cm_small_title>span {border-left: 0.075rem solid #dc481e;display: inline-block;padding-left: 0.375rem;line-height: 0.4rem;}
.i_unMenu .i_main {bottom: 0;}
/*personalletter_news_search*/
.i_h_gb_search {background: rgba(255,255,255,.3);border-radius: 0.5rem;display: block;height: 0.78rem;padding-right: 0.5rem;overflow: hidden;clear: both;}
.i_h_gb_s_ico {display: inline-block;width: 0.60rem;height: 0.60rem;vertical-align: middle;margin:0 0.20rem;background: url("../img/shear-w.svg") no-repeat 50% 50%;background-size: 100% 100%;}
.i_h_gb_s_input {background: none;border: none;display: inline-block;width: 80%;height: 0.78rem;line-height: 0.78rem;font-size: 0.35rem;color: #fff;vertical-align: middle;}
.i_h_gb_s_input::-webkit-input-placeholder { color: #f2f2f2;}
.i_news_seach_box{padding: 0.2rem 0.5rem 0 0.98rem;}
.news_seach_navBtn{position: absolute;top:0.2rem;left: 0.275rem;}
.i_cm_goBack_btn {display: inline-block;width: 0.8rem;height: 0.8rem;vertical-align: middle;background: url("../img/back.svg") no-repeat 50% 50%;background-size: 100% 100%;}
/*seach_group ---style*/
.seach_group_ul li{border-bottom: 0.025rem solid #f2f2f2;padding: 0.25rem 0;position: relative;}
.i_cm_close{position: absolute;top:0.25rem;right: 0.1rem;}
.g_nub_list{width: 0.5rem;height: 0.5rem;text-align: center;line-height: 0.5rem;background: #dddddd;color: #fff;font-size: 0.3rem;display: inline-block;margin-right:0.25rem;}
.seach_group_ul li:nth-child(1) .g_nub_list{background: #e3541c;}
.seach_group_ul li:nth-child(2) .g_nub_list{background: #ec7358;}
.seach_group_ul li:nth-child(3) .g_nub_list{background: #f5b352;}
.list_search_ioc{background: url("../img/shear-h.svg") no-repeat 50% 50%;background-size: 80% 80%;}
.close_ico{background: url("../img/close.svg") no-repeat 50% 50%;background-size: 80% 80%;}

#foot{background-color: #38404e;padding: 0.5rem 0;}
.lx-txtwarp p,.lx-txtwarp p a,.ewm-box p{line-height: 0.75rem;font-size: 0.35rem;color: #ddd;}
.ewm-box{margin-top:0.25rem;text-align: center;}
.ewm-pic{display: inline-block;width: 200px;height: 200px;}
.ewm-pic img{display: block;width: 100%;height: 100%;}