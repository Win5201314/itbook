<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zsl.boss.demo.mapper.BaseMapper">
    <update id="updateHeadimgurl" useGeneratedKeys="true" keyProperty="id">
        <if test="type == 1">
          update users set headimgurl = #{headimgurl} where id = #{id}
        </if>
        <if test="type == 2">

        </if>
    </update>
</mapper>